{
  "name": "miniorange/scim-laravel",
  "description": "Laravel SCIM Solution allows you to perform real-time user provisioning/sync from your Identity Provider to Service Provider.",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "miniOrange Security Software Pvt Ltd",
      "email": "laravelsupport@xecurify.com"
    }
  ],
  "require": {
    "php": "^5.1 || ^7.1 || ^8.0",
    "laravel/framework": ">=5.2"
  },
  "autoload": {
    "files": [
      "src/connector.php"
    ],
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "provider\\": "provider/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "provider\\ScimServiceProvider"
      ]
    }
  }
}
